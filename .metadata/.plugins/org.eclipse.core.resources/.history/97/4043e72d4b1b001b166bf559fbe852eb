#include <iostream>

using namespace std;

int main(){

	//Değişken Tipleri ve Değişkenler
	/*
	 	 int
	 	 float
	 	 double
	 	 char
	 	 bool
	 */

	int a = 32;
	float b = 3.52;
	double c = 4.546;
	char d = 'A';
	bool e = false;

	cout << "a değişkeninin değeri : " << a << endl;
	cout << "b değişkeninin değeri : " << b << endl;
	cout << "c değişkeninin değeri : " << c << endl;
	cout << "d değişkeninin değeri : " << d << endl;

	return 0;
}
