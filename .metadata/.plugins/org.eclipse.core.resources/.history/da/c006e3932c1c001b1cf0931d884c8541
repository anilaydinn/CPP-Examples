#include <iostream>

using namespace std;

class Employee{

private:
	string name;
	int id;
	int salary;

	void showInfos(){
		cout << "Id: " << id << endl;
		cout << "İsim: " << name << endl;
		cout << "Maaş: " << salary << endl;
	}
};

int main(){

	Employee employee;

	employee.id = 12;
	employee.name = "Anıl";
	employee.salary = 500;

	return 0;
}
