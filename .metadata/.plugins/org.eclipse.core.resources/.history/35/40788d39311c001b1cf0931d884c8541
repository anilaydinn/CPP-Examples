#include <iostream>

using namespace std;

class Employee{

private:
	string name;
	int age;

public:

	void setAge(int yas){
		age = yas;
	}
	int getAge(){
		return age;
	}
	void setName(string isim){
		name = isim;
	}
	string getName(){
		return name;
	}
};

int main(){

	Employee employee;

	employee.setName("AnÄ±l");
	employee.setAge(22);

	cout << employee.getName() << endl;
	cout << employee.getAge() << endl;

	return 0;
}
