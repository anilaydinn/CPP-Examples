#include <iostream>

using namespace std;

class Employee{

private:
	string name;
	int salary;

public:

	Employee(string name, int salary){

	}

	void setName(string isim){
		name = isim;
	}
	string getName(){
		return name;
	}
	void setSalary(int maas){
		salary = maas;
	}
	int getSalary(){
		return salary;
	}
};

int main(){

	Employee employee;



	return 0;
}
