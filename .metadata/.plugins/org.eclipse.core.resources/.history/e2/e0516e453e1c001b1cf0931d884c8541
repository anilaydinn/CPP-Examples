#include <iostream>

using namespace std;

class Employee{

private:
	string name;
	int age;
	int salary;

public:
	Employee(string name, int age, int salary){
		this->name = name;
		this->age = age;
		this->salary = salary;
	}
};

void showInfos(Employee employee){
	cout << employee.name << " " << employee.age << " " << employee.salary;
}

int main(){



	return 0;
}
